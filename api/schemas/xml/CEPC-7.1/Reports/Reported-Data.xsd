  <xs:element name="ACI-Cooling-Plant-Inspection"
              type="ACI-Cooling-Plant-Inspection"/>
  <xs:complexType name="ACI-Cooling-Plant-Inspection">
    <xs:sequence>
      <xs:element name="Heat-Rejection-Operational" type="Flag_With_Note">
        <xs:annotation>
          <xs:documentation>Is heat rejection plant operational?
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Heat-Rejection-Area-Clear" type="Flag_With_Note">
        <xs:annotation>
          <xs:documentation>Is area around the heat rejection plant clear of
            obstructions &amp; debris?

            If "No" then provide information and description of obstructions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Chiller-Plant-Operational" type="Flag_With_Note"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>Is chiller plant operational?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Chiller-Plant-Area-Clear" type="Flag_With_Note"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>Is area around the Chiller Plant clear of
            obstructions &amp; debris?

            If "No" then provide information and description of obstructions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Air-Recirculation" type="Flag_With_Note">
        <xs:annotation>
          <xs:documentation>Is there any possibility of air recirculation
            through the condenser?

            If "Yes" then provide information and description of obstructions
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Chiller-Plant-OK" type="Flag_With_Note" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Is general condition of chiller and associated
            central plant in good order?

            If "No" then provide information and description of dilapidations.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Heat-Exchanger-OK" type="Flag_With_Note">
        <xs:annotation>
          <xs:documentation>Are condenser heat exchangers undamaged/ un-corroded
            and clean?

            If "No" then provide information and description of dilapidations.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Pipe-Insulation-OK" type="Flag_With_Note">
        <xs:annotation>
          <xs:documentation>Is insulation on primary circulation pipe work well
            fitted and in good order?

            If "No" then provide information and description of dilapidations.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Chiller-Placement-OK" type="Flag_With_Note"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>Is the chiller unit placed clear from warm air
            discharge louvers

            If "No" then provide information and description of dilapidations.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Compressors-Operational" type="Flag_With_Note">
        <xs:annotation>
          <xs:documentation>Locate compressors and ensure they are operational
            or can be brought into operation

            If "No" then provide information and description of dilapidations.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
